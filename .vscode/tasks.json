{
    "version": "2.0.0",
    "tasks": [{
        "label": "build",
        "type": "shell",
        "command": "sjasmplus",
        "args": [
            "--fullpath",
            "--sld=./output/${fileBasenameNoExtension}.sld",
            "${relativeFile}"
        ],
        "problemMatcher": ["$problem-matcher-sjasmplus"],
        "group": {
            "kind": "build",
            "isDefault": true
        }
    }]
}